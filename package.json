{
  "name": "jsplumb-react",
  "title": "jsPlumb React",
  "license": "GPL-3.0",
  "version": "0.0.0-semantic-release",
  "description": "A react wrapper for jsPlumb",
  "author": "wallzero @wallzeroblog (http://wallzero.com)",
  "contributors": [
    {
      "name": "Connor Schlesiger",
      "email": "connor@schlesiger.ca",
      "position": "Initial and Lead Developer"
    }
  ],
  "main": "lib/main.min.js",
  "types": "index",
  "files": [
    "index.d.ts",
    "lib/"
  ],
  "homepage": "https://gitlab.com/wallzero/jsplumb-react",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/wallzero/jsplumb-react.git"
  },
  "bugs": {
    "url": "https://gitlab.com/wallzero/jsplumb-react/issues"
  },
  "scripts": {
    "build": "webpack --display-error-details",
    "build:dev": "cross-env NODE_ENV='development' webpack --progress --display-error-details",
    "build:demo": "cross-env DIGEST='digest.demo.config.json' webpack --display-error-details",
    "build:demo:dev": "cross-env DIGEST='digest.demo.config.json' NODE_ENV='development' webpack --progress  --display-error-details",
    "build:demo:gitlab": "cross-env BASE_HREF='/jsplumb-react/' DIGEST='digest.demo.config.json' webpack --display-error-details",
    "build:demo:dev:gitlab": "cross-env BASE_HREF='/jsplumb-react/' DIGEST='digest.demo.config.json' NODE_ENV='development' webpack --progress  --display-error-details",
    "---------------------------------------------------------------------": "",
    "start:demo": "cross-env DIGEST='digest.demo.config.json' node node_modules/@digest/webpack-express/src/start.js || true",
    "start:demo:dev": "cross-env DIGEST='digest.demo.config.json' NODE_ENV='development' node node_modules/@digest/webpack-express/src/start.js || true",
    "start:demo:gitlab": "cross-env BASE_HREF='/jsplumb-react/' DIGEST='digest.demo.config.json' node node_modules/@digest/webpack-express/src/start.js || true",
    "start:demo:dev:gitlab": "cross-env BASE_HREF='/jsplumb-react/' DIGEST='digest.demo.config.json' NODE_ENV='development' node node_modules/@digest/webpack-express/src/start.js || true",
    "--------------------------------------------------------------------": "",
    "test": "run-p lint unit",
    "lint": "run-p lint:ts lint:css",
    "lint:ts": "tslint 'src/**/*.ts?(x)' 'demo/**/*.ts?(x)'",
    "lint:css": "run-p lint:css:src lint:css:demo",
    "lint:css:src": "stylelint 'src/**/*.css'",
    "lint:css:demo": "stylelint 'demo/**/*.css'",
    "unit": "run-s clean:coverage 'jest --coverage --ci'",
    "unit:ci": "run-s clean:coverage 'jest --coverage --ci'",
    "unit:dev": "run-s clean:coverage 'jest --watch --no-cache'",
    "jest": "jest --config jest.config.js --color",
    "-------------------------------------------------------------------": "",
    "e2e": "run-s e2e:setup e2e:start:all",
    "e2e:start:all": "run-p --race start:demo e2e:nightwatch:all",
    "e2e:nightwatch:all": "sleep 3 && REPORT_PATH='reports/demo' nightwatch --config node_modules/@digest/nightwatch/src/nightwatch.config.js --env chrome,firefox",
    "e2e:setup": "run-p e2e:build e2e:install",
    "e2e:build": "tsc --project demo/__e2e__/tsconfig.json",
    "e2e:install": "node node_modules/@digest/nightwatch/src/install.js",
    "-----------------------------------------------------------------": "",
    "e2e:chrome": "run-s e2e:setup e2e:build e2e:start:chrome",
    "e2e:start:chrome": "run-p --race start:demo e2e:nightwatch:chrome",
    "e2e:nightwatch:chrome": "sleep 3 && HEADLESS='false' REPORT_PATH='reports/demo' nightwatch --config node_modules/@digest/nightwatch/src/nightwatch.config.js -e chrome",
    "e2e:chrome:gitlab": "run-s e2e:setup e2e:build e2e:start:chrome:gitlab",
    "e2e:start:chrome:gitlab": "run-p --race start:demo:gitlab e2e:nightwatch:chrome:gitlab",
    "e2e:nightwatch:chrome:gitlab": "sleep 3 && BASE_HREF='/jsplumb-react/' nightwatch --config node_modules/@digest/nightwatch/src/nightwatch.config.js -e chrome",
    "----------------------------------------------------------------": "",
    "e2e:firefox": "run-s e2e:setup e2e:build e2e:start:firefox",
    "e2e:start:firefox": "run-p --race start:demo e2e:nightwatch:firefox",
    "e2e:nightwatch:firefox": "sleep 3 && HEADLESS='false' REPORT_PATH='reports/demo' nightwatch --config node_modules/@digest/nightwatch/src/nightwatch.config.js -e firefox",
    "e2e:firefox:gitlab": "run-s e2e:setup e2e:build e2e:start:firefox:gitlab",
    "e2e:start:firefox:gitlab": "run-p --race start:demo:gitlab e2e:nightwatch:firefox:gitlab",
    "e2e:nightwatch:firefox:gitlab": "sleep 3 && BASE_HREF='/jsplumb-react/' nightwatch --config node_modules/@digest/nightwatch/src/nightwatch.config.js -e firefox",
    "---------------------------------------------------------------": "",
    "copy": "run-p copy:coverage copy:stats",
    "copy:coverage": "cpx \"reports/lib/coverage/lcov-report/**/*\" public/coverage",
    "copy:stats": "cpx \"reports/lib/webpack/**/*\" public/webpack",
    "------------------------------------------------------------------": "",
    "clean": "run-p clean:node_modules clean:coverage clean:e2e clean:misc clean:stats",
    "clean:node_modules": "rimraf node_modules yarn.lock yarn-error.log package-lock.json npm-debug.log",
    "clean:coverage": "rimraf reports/coverage public/coverage",
    "clean:e2e": "rimraf reports/demo/e2e",
    "clean:misc": "rimraf lib public",
    "clean:stats": "rimraf reports/lib/webpack reports/demo/webpack public/webpack",
    "commit": "git-cz",
    "release:gitlab": "semantic-release",
    "release:npm": "npm-publish-git-tag"
  },
  "dependencies": {
    "jsplumb-react": "^0.0.27",
    "react-pan-and-zoom-hoc": "1.1.11"
  },
  "peerDependencies": {
    "jsplumb": "^2.8.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "uuid": "^3.3.0"
  },
  "devDependencies": {
    "@digest/jest": "^1.1.0",

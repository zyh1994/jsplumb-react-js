    "@types/react": "^16.4.12",
    "@types/react-css-modules": "^4.6.2",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-sidebar": "^2.2.3",
    "@types/react-swipeable-views": "^0.12.2",
    "@types/react-test-renderer": "^16.0.2",
    "@types/react-virtualized": "^9.18.7",

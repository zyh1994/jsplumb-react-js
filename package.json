    "enzyme": "^3.5.0",
    "enzyme-adapter-react-16": "^1.3.0",
    "flying-focus-x": "^1.0.0",
    "jsdom": "^12.0.0",
    "jsplumb": "^2.9.3",
    "lodash.debounce": "^4.0.8",
    "npm-publish-git-tag": "^2.1.16",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-axe": "^3.0.1",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.4.2",
    "react-virtualized": "^9.20.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "~2.13.5",
    "redux-immutable-state-invariant": "^2.1.0",
    "rimraf": "^2.6.2",
    "selenium-standalone": "^6.15.2",
    "semantic-release": "^15.9.6",
    "ui-router-react-digest": "^1.1.1",
    "uuid": "^3.3.2",
    "why-did-you-update": "^0.1.1"
  },
  "keywords": [
    "react",
    "reactjs",
    "jsplumb",
    "dag"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}

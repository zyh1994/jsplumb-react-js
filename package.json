    "@digest/webpack-babel": "^1.1.0",
    "@digest/webpack-express": "^1.1.0",
    "@digest/webpack-postcss": "^1.1.0",
    "@digest/webpack-react": "^1.1.0",
    "@digest/webpack-stylelint": "^1.1.0",
    "@digest/webpack-tslint": "^1.1.0",
    "@digest/webpack-typescript": "^1.1.0",
    "@digest/webpack-workbox": "^1.1.0",
    "@semantic-release/gitlab": "^3.0.1",
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.1",
    "@types/jsdom": "^11.0.6",
    "@types/lodash.debounce": "^4.0.4",
    "@types/nightwatch": "~0.9.8",
    "@types/node": "^10.9.3",
